{
  "name": "@ethersphere/swarm-extension",
  "version": "0.1.0",
  "description": "JavaScript library for interaction with the Swarm browser extension",
  "main": "build/index.js",
  "types": "build/types/index.d.ts",
  "scripts": {
    "start": "webpack --watch --env mode=development",
    "build": "webpack --env mode=production",
    "serve": "http-server",
    "lint": "eslint --resolve-plugins-relative-to . --fix \"src/**/*.ts\" \"test/**/*.ts\" && prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint:check": "eslint --resolve-plugins-relative-to . \"src/**/*.ts\" \"test/**/*.ts\" && prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "jest --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nugaon/swarm-extension.git"
  },
  "keywords": [
    "Swarm",
    "extension",
    "browser",
    "library"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nugaon/swarm-extension/issues"
  },
  "homepage": "https://github.com/nugaon/swarm-extension#readme",
  "dependencies": {
    "@types/chrome": "^0.0.196",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.16",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.0.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-webpack-plugin": "^3.2.0",
    "jest": "^29.0.2",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.0",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
